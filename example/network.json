{
  "inputs": [{
    "name": "image",
    "type": "int",
    "to": ["layer1"]
  }, {
    "name": "label",
    "type": "list",
    "to": []
  }],
  "layers": [{
    "name": "layer1",
    "type": "Conv",
    "activation": "Relu",
    "filter": [3, 3, 3, 64],
    "strides": [1, 1, 1, 1],
    "padding": "SAME",
    "to": ["layer2"]
  }, {
    "type": "Conv",
    "name": "layer2",
    "activation": "Relu",
    "filter": [1, 2, 2, 1],
    "strides": [1, 1, 1, 1],
    "padding": "SAME"
  }],
  "configure": {
    "framework": "tensoflow"
  }
}